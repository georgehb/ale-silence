Execute:
  function! EntryFor(name)
    let name = a:name
    function! Entry(bufnr, lnum, code) closure
      return {
        \ 'bufnr': a:bufnr,
        \ 'lnum':  a:lnum,
        \ 'code':  a:code,
        \ 'linter_name': name
        \ }
    endfun
    return funcref('Entry')
  endfun

  function! SetBufferInfo(bufnr, ...)
    let g:ale_buffer_info = {
    \ a:bufnr : { 'loclist': a:000 }
    \ }
  endfun
